{
  "name": "moes-card-room",
  "version": "1.1.0",
  "description": "Multiplayer War card game with lobby, chat, Google login, and card room automation.",
  "main": "server.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "test": "node test-regression.js",
    "test:db": "node test-db.js",
    "test:games": "ts-node test/WarEngine.test.ts",
    "test:all": "npm run test:games && npm run test",
    "test:automated": "powershell -ExecutionPolicy Bypass -File ./run-automated-tests.ps1",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:test": "node test-db.js",
    "worker:payout": "ts-node src/queue/payout.worker.ts",
    "build:ts": "tsc --project tsconfig.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@dicebear/collection": "^9.2.4",
    "@dicebear/core": "^9.2.4",
    "@prisma/client": "^5.22.0",
    "@simplewebauthn/browser": "^13.2.2",
    "@simplewebauthn/server": "^13.2.2",
    "@socket.io/redis-adapter": "^8.3.0",
    "@upstash/redis": "^1.35.7",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.65.1",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "express-session": "^1.17.3",
    "memorystore": "^1.6.7",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "prisma": "^5.22.0",
    "redis": "^5.10.0",
    "redlock": "^5.0.0-beta.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
